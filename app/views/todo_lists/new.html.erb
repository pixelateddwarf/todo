<div class="booyah-box col-xs-10 col-xs-offset-1">
  <h1>New todo_list</h1>

  <%= simple_form_for @todo_list do |f| %>
    <%= f.input :title %>
    <%= f.input :description %>
    <br />
    <%= f.submit 'Create', :class => 'btn btn-primary' %>
  <% end %>

  <%= link_to 'Back', todo_lists_path %>
</div>
